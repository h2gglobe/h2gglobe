## Di-jet tag
includeVBF=1
nVBFEtaCategories=1
nVBFDijetJetCategories=2
leadEtVBFCut=60
subleadEtVBFCut=25

usePUjetveto=1

## Singal mass points to book
sigPointsToBook=105,110,115,120,125,130,135,140,145,150

## Hadronic V tag
includeVHhad=0
nVHhadEtaCategories=1
leadEtVHhadCut=60
subleadEtVHhadCut=25

## Lepton tag
includeVHlep=0
leadEtVHlepCut=45
subleadEtVHlepCut=25

## MET tag
includeVHmet=0
leadEtVHmetCut=45
subleadEtVHmetCut=25

## Inputs for reweighing
doKFactorSmear=0
doKFactorSyst=0
kfacHist=
## For 7TeV samples
## kfacHist=aux/KFactors_AllScales_interpolated.root
puMap=
puHist=auto
puTarget=aux/latest_prompt_NoLowPU_ICHEP_535.json.71000.observed.pileup.root

## energy regression
energyCorrectionMethod=Bendavid
doEcorrectionSmear=1
doEcorrectionSyst=0

## energy scale adjustement and errors
scale_offset_file=energy_scale_offsets.dat
scale_offset_error_file=energy_scale_errors.dat

## additional MC smearing
smearing_file=smearing_sigma_and_errors.dat

## jets
recomputeBetas=0
recorrectJets=1
rerunJetMva=0
recomputeJetWp=1

## revertex
reRunVtx=0
rematchConversions=1

doInterferenceSmear=1

